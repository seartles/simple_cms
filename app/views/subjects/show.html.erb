<% if !flash[:notice].blank? %>
    <div class="notice">
      <%= flash[:notice]%>
    </div>
<% end %>
<%= link_to "<<Back to list", {:action=>'index', :class=> 'back-link'}%>
<div class=" Subject show">
  <h2>Show Subject</h2>
  <table>
    <tr>
      <th>Name</th>
      <td><%=@subject.name%></td>
    </tr>
    <tr>
      <th>Position</th>
      <td><%=@subject.position%></td>
    </tr>
    <tr>
      <th>Visible</th>
      <td><%=@subject.visible ? 'Yes': 'No' %></td>
    </tr>
    <tr>
      <th>Created</th>
      <td><%=@subject.created_at%></td>
    </tr>
    <tr>
      <th>Updated</th>
      <td><%=@subject.updated_at%></td>
    </tr>
  </table>
</div>
